<Application
    x:Class="WorkspaceLauncher.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:sys="clr-namespace:System;assembly=mscorlib"
    xmlns:system="clr-namespace:System;assembly=System.Runtime"
    StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="Primary">#59baeb</SolidColorBrush>
        <SolidColorBrush x:Key="Secondary">LightGreen</SolidColorBrush>
        <SolidColorBrush x:Key="Tertiary">Yellow</SolidColorBrush>
        <FontFamily x:Key="PrimaryFontFamily">Segoe UI Light</FontFamily>
        <sys:Double x:Key="PrimaryFontSize">30</sys:Double>
        <!--  Application Brand Name  -->
        <system:String x:Key="AppBrandName">WorkspaceLauncher</system:String>
    </Application.Resources>
</Application>
